{
  "name": "error-insights-frontend",
  "version": "1.0.0",
  "description": "Real-Time Error Insights Dashboard Frontend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:prod": "ng serve --configuration production"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.5",
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.2.5",
    "@angular/compiler": "^20.2.5",
    "@angular/core": "^20.2.5",
    "@angular/forms": "^20.2.5",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.2.5",
    "@angular/platform-browser-dynamic": "^20.2.5",
    "@angular/router": "^20.2.5",
    "chart.js": "^4.4.0",
    "date-fns": "^3.0.6",
    "lodash-es": "^4.17.21",
    "ng2-charts": "^7.0.0",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-devkit/build-angular": "^20.2.5",
    "@angular/build": "^20.2.5",
    "@angular/cli": "^20.2.5",
    "@angular/compiler-cli": "^20.2.5",
    "@types/jest": "^30.0.0",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^15.0.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.4",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
